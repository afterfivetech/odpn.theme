<?xml version="1.0"?>
<registry>
    <records prefix="plone.resources/odpn-main"
              interface='Products.CMFPlone.interfaces.IResourceRegistry'>
       <value key="css">
          <element>++theme++odpn.theme/css/main.less</element>
       </value>
    </records>

    <!-- bundle definition -->
    <records prefix="plone.bundles/odpn-bundle"
              interface='Products.CMFPlone.interfaces.IBundleRegistry'>
      <value key="resources">
        <element>odpn-main</element>
      </value>
      <value key="enabled">True</value>
      <value key="compile">True</value>
      <value key="csscompilation">++theme++odpn.theme/css/odpn-compiled.css</value>
      <value key="last_compilation"></value>
    </records>
</registry>